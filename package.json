{
  "name": "webpack2-conf-test",
  "version": "1.0.0",
  "description": "webpack2在项目中的配置实践",
  "repository": "git@github.com:TaylorPzreal/webpack2-conf-test.git",
  "author": "TaylorPzreal <taylorpzreal@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --port 3000 --progress --profile --colors",
    "start": "yarn run dev -- --hot",
    "start:dash": "webpack-dashboard -- cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --hot --profile",
    "start:browser": "yarn run start -- --open",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js --progress --profile --colors",
    "localserver": "http-server ./ -p 3000"
  },
  "dependencies": {
    "angular": "^1.6.2",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.1",
    "eslint-loader": "^1.6.3",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.1",
    "precss": "^1.4.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.5",
    "uglifyjs-webpack-plugin": "^0.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^3.0.0"
  }
}
